{
  "name": "ngx-long-press",
  "version": "0.0.1",
  "description": "Long Press directive for Angular",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "build": "webpack && npm run tsc && npm run ngc && npm run moveassets",
    "watch": "webpack -- --watch",
    "server": "webpack-dev-server --config webpack.demo.js --inline --colors --progress --port 3015 --content-base demo",
    "start": "npm run server",
    "test": "karma start karma.conf.js",
    "tsc": "tsc -p tsconfig.json",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "moveassets": "rsync -a --include='*.scss' --include='*.html' --include='*.metadata.json' --include='*/' --exclude='*' src/ dist/src/",
    "codecov": "codecov",
    "clean": "npm run clean:dist && npm run clean:generated",
    "clean:dist": "rm -rf ./dist && rm -rf ./factories",
    "clean:generated": "find src \\( -name '*.js' -o -name '*.d.ts' -o -name '*.js.map' -o -name '*.metadata.json'  \\) -type f -delete"
  },
  "author": {
    "name": "Giancarlo Buomprisco",
    "email": "giancarlopsk@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^4.3.0",
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/compiler-cli": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@types/es6-shim": "^0.31.32",
    "@types/jasmine": "^2.5.41",
    "@types/node": "^7.0.3",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "^6.7.0",
    "awesome-typescript-loader": "3.1.3",
    "codecov": "^2.1.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.1",
    "es6-promise": "4.1.0",
    "es6-shim": "0.35.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.4",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine": "^2.5.3",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.3",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.3.1",
    "sass-loader": "^6.0.1",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.17.0",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "1.1.2",
    "ts-node": "^3.0.2",
    "tslint": "^5.2.0",
    "typescript": "2.3.2",
    "universal-style-loader": "^0.16.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.0",
    "webpack-dev-server": "https://registry.npmjs.org/webpack-dev-server/-/webpack-dev-server-2.4.2.tgz",
    "webpack-merge": "^4.1.0",
    "zone.js": "0.8.10"
  },
